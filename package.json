{
  "name": "browserifyjs",
  "version": "1.0.0",
  "description": "using Browserify just for fun :-)",
  "main": "main.js",
  "scripts": {
    "build-js": "browserify frameworks/vanillajs/main_1.js > frameworks/vanillajs/main-min.js -dv",
    "watch-js": "watchify frameworks/vanillajs/main_1.js -o frameworks/vanillajs/main-min.js -dv",
    "start-js": "beefy frameworks/vanillajs/main_1.js:frameworks/vanillajs/main-min.js --live",
    "build-backbone-js": "browserify frameworks/backbone/main.js > frameworks/backbone/main-min.js -dv"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OmerHerera/browserifyjs.git"
  },
  "keywords": [
    "browserify",
    "tutorial",
    "browserifyjs"
  ],
  "author": "Omer Herera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OmerHerera/browserifyjs/issues"
  },
  "homepage": "https://github.com/OmerHerera/browserifyjs",
  "dependencies": {
    "backbone": "^1.2.1",
    "jquery": "^2.1.4",
    "react": "^0.13.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "beefy": "^2.1.5",
    "browserify": "^10.2.4",
    "grunt": "^0.4.5",
    "grunt-available-tasks": "^0.5.7",
    "grunt-browserify": "^3.8.0",
    "grunt-contrib-watch": "^0.6.1",
    "load-grunt-config": "^0.17.1",
    "time-grunt": "^1.2.1",
    "watchify": "^3.2.3"
  }
}
